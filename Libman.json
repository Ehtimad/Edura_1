{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "unpkg",
      "library": "popper.js@1.14.4",
      "destination": "wwwroot/lib/popper.js/"
    },
    {
      "provider": "unpkg",
      "library": "tether@1.4.5",
      "destination": "wwwroot/lib/tether/"
    },
    {
      "provider": "unpkg",
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "provider": "unpkg",
      "library": "ckeditor@4.11.1",
      "destination": "wwwroot/lib/ckeditor"
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation@1.19.0",
      "destination": "wwwroot/lib/JQueryValidator"
